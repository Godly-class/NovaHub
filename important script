    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Vynixius UI è¼‰å…¥å¤±æ•—",
        Text = "éŒ¯èª¤: " .. tostring(Vynixius) .. "\nè«‹æª¢æŸ¥ç¶²è·¯æˆ– executor",
        Duration = 10
    })
    return
end

