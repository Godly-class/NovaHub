local themes = {
    "Dark ğŸŒ™", 
    "Light â˜€ï¸", 
    "Darker ğŸŒ‘", 
    "Luna ğŸŒ•", 
    "Aqua ğŸŒŠ",
    "Purple ğŸ’œ",
    "Red â¤ï¸"
