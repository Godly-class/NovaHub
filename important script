SettingsTab:Dropdown({
    Title = "ğŸ­ ä»‹é¢ä¸»é¡Œ",
    Desc = "æ›´æ”¹ä»‹é¢ä¸»é¡Œèˆ‡é…è‰²",
    Values = themes,
    Value = "Dark ğŸŒ™",
    Callback = function(value)
        local themeName = string.gsub(value, " [%p%w]*$", "")
        WindUI:SetTheme(themeName)
        showNotification("ğŸ­ ä¸»é¡Œ", "ä»‹é¢ä¸»é¡Œå·²è¨­ç‚ºï¼š" .. value, 3, "palette")
    end
})

